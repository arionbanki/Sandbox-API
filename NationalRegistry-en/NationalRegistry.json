              },
              "home": {
                "description": "The legal residence of the party in question.",
                "type": "string"
              },
              "kennitala": {
                "description": "Kennitala úr Þjóðskrá.",
                "type": "string"
              },
              "postalCode": {
                "description": "The postal code of the residence.",
                "type": "string"
              }
            },
            "required": [
              "kennitala",
              "fullName",
              "home",
              "postalCode",
              "city",
              "fate"
            ],
            "type": "object"
          },
          "type": "array"
        },
        "paging": {
          "properties": {
            "totalPages": {
              "type": "integer"
            },
            "totalRecords": {
              "type": "integer"
            }
          },
